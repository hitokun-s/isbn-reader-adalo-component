{
  "displayName": "ISBN Reader",
  "defaultWidth": 160,
  "defaultHeight": 24,
  "components": "./index.js",
  "icon": "./example-thumbnail.png",
  "props": [
    {
      "name": "text",
      "displayName": "Text",
      "type": "text",
      "default": "Happy Hacking!"
    },
    {
      "name": "color",
      "displayName": "Color",
      "type": "color",
      "default": "#00A898"
    },
    {
      "name": "barcodeType",
      "displayName": "barcode type",
      "type": "text",
      "default": "general",
      "control": {
        "type": "string",
        "options": [
          { "label": "general", "value": "general" },
          { "label": "ISBN", "value": "ISBN" }
        ]
      }
    }
  ]
}
